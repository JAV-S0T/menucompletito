void main (){
  animate (menu);
  
}



void menu(){
  imagen_pr();
   boton_cr();
 	 boton_ins();
 		boton_ju();
  texto_cr();
  texto_ins();
  texto_ju();
  cambiocolor_cr();
  cambiocolor_ins();
  cambiocolor_ju();
   diddy ();
  funcion_cr();
  funcion_ins();
  funcion_ju();
  
}
   
  


void imagen_pr(){
  background(255,255,255);
  image ("https://i.namu.wiki/i/VOBS787L04ZX4C-oxJeD8L_f1mjlsoV0KlC_nrUmninQixbLLAkNF6m3Iysj29F0r83zjl9z06ACyCClYQX_5A.webp",0,0,320,320);
}

void boton_cr(){
   fill (r1,g1,b1);
  rect (20,20,110,30);
  }

void boton_ins(){
  fill(r2,g2,b2);
  rect(190,20,110,30);
}

void boton_ju(){
  fill(r3,g3,b3);
  rect (110,270,110,30);
}

void cambiocolor_cr (){
  if (mouseX > 20 && mouseX < 130 && mouseY > 20 && mouseY < 50){
  	r1 = 178;
    g1 = 56;
    b1 = 187;
    
    boton_cr();
    
    fill(0,0,0);
      texto_cr();
  }
  else {
    r1 =0;
    b1 = 0;
    g1 = 0;
  }
}

void cambiocolor_ins (){
  if (mouseX > 190 && mouseX < 300 && mouseY > 20 && mouseY < 50){
  	r2 = 242;
    g2 = 10;
    b2 = 94;
    
    boton_ins();
    
    fill(0,0,0);
      texto_ins();
  }
  else{
    r2 = 0;
     b2 =0;
    g2 = 0;
  }
}

void cambiocolor_ju (){

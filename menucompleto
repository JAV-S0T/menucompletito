void puntitos (){
  int b = millis()/1000;
    if (b <= 60 ){
      puntos = puntos + random(2);}
  
}

int r4=0;
int g4=0;
int b4=0;


void perder (){
  image ("https://encrypted-tbn0.gstatic.com/images?q=tbn:ANd9GcS248mX0v4FNTFBlt4sDNxpLFGsEACtTy7VsQ&s",0,0,320,320);
  fill(0,0,0);
  textSize (18);
  text ("Perdiste",10,30);
}


void textopuntos(){
  text("puntos: " + puntos,15, 310);
}

void teclas(){
  
    if (juy > 3 &&keyPressed && key == "up"){
      juy = juy-8;
    }
  
		if (jux > 3 && keyPressed && key == "left"){
      jux = jux-8;
    }
  
		if (juy < 290 &&keyPressed && key == "down"){
      juy = juy +8;
    }
  
		if (jux < 290 && keyPressed && key == "right"){
      jux =jux + 8;
    }

	jugador();
}


void jugador(){
  fill(0,0,0);
  image ("https://preview.redd.it/fluff-make-the-comments-on-this-post-look-like-battle-cats-v0-2fyrec945rzc1.png?width=640&crop=smart&auto=webp&s=f6b7850d5eac3c5e063f0bc1657c1b704132462b",jux,juy,25,25);
 
}

void funcion_cr(){
  if (mousePressed){
    if (mouseButton == LEFTBUTTON){
      if (mouseX > 20 ){
        if (mouseX < 130){
          if (mouseY > 20){
        		if (mouseY < 50);
      
  					animate (cr);
          }}}}}}

void funcion_ins(){
  if (mousePressed){
    if (mouseButton == LEFTBUTTON){
      if (mouseX > 190 ){
        if (mouseX < 300){
          if (mouseY > 20){
        		if (mouseY < 50);
      
  					animate (ins);
          }}}}}}

void funcion_ju(){
  if (mousePressed){
    if (mouseButton == LEFTBUTTON){
      if (mouseX > 111 ){
        if (mouseX < 220){
          if (mouseY > 270){
        		if (mouseY < 300);
  					animate (ju);
          }}}}}}

void boton_volver (){
   fill(0,0,0);
  rect (0,310,50,45);
    fill(203,43,43);
  textSize (8);
    text ("Volver",10, 320 );
}

void funcion_boton_volver(){
  if (mousePressed){
    if (mouseButton == LEFTBUTTON){
      if (mouseX > 2 ){
        if (mouseX < 48){
          if (mouseY > 310){
        		if (mouseY < 320);
            animate (menu);
          }}}}}}

void tiempo(){
  int b = millis()/1000;
  fill(0,0,0);
 text("tiempo: "+ b, 230, 23 );
  
   
  }
 
void ganar(){
  int b = millis()/1000;
  if(b >= 60){
     image ("https://encrypted-tbn0.gstatic.com/images?q=tbn:ANd9GcSE4nw_ZJ6YbjsradCGTCd5t78j9J8mVUodzxLEhdGwvc4rFnKeIrKxTwUQ4xsFX3qWH7w&usqp=CAU",0,0,320,320);
    fill(231,239,28);
    text ("Ganaste!",220,140);
    boton_volver();
    funcion_boton_volver();
  }}

String nombre;

void name (){
  
  nombre = readString("Dime tu nombre");
	print("Hola ");
	print(nombre);
	println(", ¿Preparado?");
}

void textonombre(){
  fill(0,0,0);
	textSize(10);
  text (nombre, 40, 20);
}



void ju(){
  image ("https://ih1.redbubble.net/image.4564533238.0563/raf,360x360,075,t,fafafa:ca443f4786.jpg",0,0,320,320);
    fill(255,255,255);
    text ("Pulsa f para jugar",45,90);{
  if (keyPressed){
    name();
    animate (jugar);
  }}
}

void proyectiles(){
  int b = millis()/1000;
  if (b >= 3){
  rect (proyzx,proyzy,30,30);
    proyzx = proyzx - 6;   
  }
  if (b >= 4){
  rect (proyvx,proyvy,30,30);
    proyvx = proyvx - 6;
  }
  if (b >= 5){
  rect  (proysx,proysy,30,30);
    proysx = proysx - 6;
  }
  if (b >= 8){
  rect (proydx,proydy,30,30);
    proydy = proydy - 10;
  }
  if (b >= 9){
  rect (160,proyfy,30,30);
    proyfy = proyfy - 10;
  }
  if (b >= 10){
  rect (proygx,proygy,30,30);
    proygy = proygy - 10;
  }
  if (b >= 11){
  rect (proyhx,proyhy,30,30);
    proyhy = proyhy - 10;
  }
  if (b >= 11){
  rect (proyjx,proyjy,30,30);
    proyjx = proyjx - 7;
  }
  if (b >= 12){
  rect (proykx,proyky,30,30);
    proykx = proykx - 8;
  }
  if (b >= 13){
  rect (proylx,proyly,30,30);
   proyly = proyly - 5;
  }
  if (b >= 14){
  rect (proymx,proymy,30,30);
    proymx = proymx - 6;
  }
  if (b >= 14){
  rect (proynx,proyny,30,30);
    proyny = proyny - 11;
  }
  if (b >= 15){
  rect (proywx,proywy,30,30);
    proywx = proywx - 13;
  }
  if (b >= 16){
  rect (proyqx,proyqy,30,30);
    proyqx = proyqx - 4;
  }
  if (b >= 17){
  rect (proyaax,proyaay,30,30);
    proyaax = proyaax + 3;
  }
  if (b >= 19){
  rect (proyox,proyoy,30,30);
    proyoy = proyoy + 7;
  }
  if (b >= 20){
  rect (proypx,proypy,30,30);
    proypx = proypx + 9;

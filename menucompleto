    proyAy = proyAy - 7;
  }
  if (b == 28){
    fill(255,94,94);
  text ("¿Preparado?",100, 30);
      
  }
  if (b == 29){
    fill(255,39,39);
  text ("¿Preparado?",100, 30);
  }
  if (b == 30){
    fill(177,0,0);
  text ("¿Preparado?",100, 30);
  }
  if (b == 31){
    fill(55,245,6);
  text ("¡Suerte!",115, 30);
  }
   if (b >= 32){
  rect (proyBx,proyBy,75,75);
    proyBx = proyBx - 1;
  }
   if (b >= 33){
  rect (proyCx,proyCy,50,50);
    proyCy = proyCy - 8;
  }
   if (b >= 33){
  rect (proyDx,proyDy,55,55);
    proyDy = proyDy + 8;
  }
   if (b >= 34){
  rect (proyRx,proyRy,35,35);
    proyRx = proyRx - 4;
  }
   if (b >= 35){
  rect (proyFx,proyFy,30,30);
    proyFy = proyFy - 3;
  }
   if (b >= 35){
  rect (proyGx,proyGy,30,30);
    proyGx = proyGx - 9;
  }
   if (b >= 38){
  rect (proyHx,proyHy,40,40);
    proyHy = proyHy + 7;
  }
   if (b >= 40){
  rect (proyIx,proyIy,25,25);
    proyIx = proyIx - 13;
  }
   if (b >= 41){
  rect (proyJx,proyJy,60,60);
    proyJy = proyJy + 7;

   if (b >= 46){
  rect (proyNx,proyNy,90,90);
    proyNx = proyNx + 5;
  }
  if (b >= 48){
  rect (proyOx,proyOy,40,40);
    proyOy = proyOy - 9;
  }
  if (b >= 50){
     fill(165,8,8);
  rect (proyPx,proyPy,50,50);
    proyPx = proyPx - 6;
  }
  if (b >= 50){
     fill(165,8,8);
  rect (proyQx,proyQy,60,60);
    proyQx = proyQx + 7;
  }
  if (b >= 51){
     fill(165,8,8);
  rect (proySx,proySy,70,70);
    proySy = proySy - 6;
  }
  if (b >= 51){
     fill(165,8,8);
  rect (proyTx,proyTy,40,40);
    proyTy = proyTy - 8;
  }
  if (b >= 53){
    fill(98,0,0);
  rect (proyUx,proyUy,65,65);
    proyUx = proyUx + 7;
  }
  if (b >= 55){
     fill(98,0,0);
  rect (proyVx,proyVy,70,70);
    proyVy = proyVy + 6;
  }
  if (b >= 55){
      fill(98,0,0);
